{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/bot-unified",
      "dest": "/api/bot-unified.js"
    },
    {
      "src": "/api/babiya",
      "dest": "/api/babiya.js"
    },
    {
      "src": "/api/ema",
      "dest": "/api/ema.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
