{
  "version": 2,
  "builds": [
    {
      "src": "/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/bot",
      "destination": "/api/bot.js"
    },
    {
      "source": "/api/cron",
      "destination": "/api/cron.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 * * * *"
    }
  ]
}
